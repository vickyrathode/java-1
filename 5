import java.util.*;
 
public class CompareString5 {

    public void compare(String s1,String s2)
    {
        if(s1.compareTo(s2)==0)
        {
            System.out.print("Strings are Equal ");
        }
        else{
            System.out.print("string are not equal");
        }
    }
    public void compare(String s1,String s2,int n){
        int flag =0;
        for(int i=0;i<n;i++)
        {
            if(s1.charAt(i) != s2.charAt(i))
            { 
                flag = 1;
            }
        }
        if(flag==0)
        {
            System.out.print("2 string are equal no "+n);
        }
        else{
            System.out.print("2 string are not equal no "+n);

        }

    }
    public static void main (String[] args)
    {
        Scanner input = new Scanner(System.in);
        CompareString5 poly = new CompareString5();
        System.out.print("enter the first String :");
        String s1 = input.nextLine();
        System.out.print("enter the second String :");
        String s2 =input.nextLine();
        poly.compare(s1,s2);

        System.out.print("\nenter length of String :");
        int num = input.nextInt();
        poly.compare(s1,s2,num);

        input.close();

    }
    
}
